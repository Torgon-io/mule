{
  "name": "@torgon/mule",
  "version": "0.1.5",
  "exports": "./main.ts",
  "license": "MIT",
  "tasks": {
    "dev": "deno run --watch main.ts",
    "query": "deno run --allow-read --allow-write --allow-env --allow-ffi --allow-net --unstable-ffi scripts/query-executions.ts",
    "export": "deno run --allow-read --allow-write --allow-env --allow-ffi --allow-net --unstable-ffi scripts/export-executions.ts",
    "stats": "deno run --allow-read --allow-write --allow-env --allow-ffi --allow-net --unstable-ffi scripts/stats.ts",
    "cleanup": "deno run --allow-read --allow-write --allow-env --allow-ffi --allow-net --unstable-ffi scripts/cleanup.ts",
    "example": "deno run --allow-read --allow-write --allow-env --allow-ffi --allow-net --unstable-ffi scripts/example.ts",
    "studio": "deno run --allow-read --allow-write --allow-env --allow-ffi --allow-net --unstable-ffi scripts/studio-server.ts"
  },
  "imports": {
    "@openrouter/ai-sdk-provider": "npm:@openrouter/ai-sdk-provider@^1.2.8",
    "@std/assert": "jsr:@std/assert@1.0.15",
    "ai": "npm:ai@^5.0.93",
    "zod": "npm:zod@^4.1.12"
  }
}
